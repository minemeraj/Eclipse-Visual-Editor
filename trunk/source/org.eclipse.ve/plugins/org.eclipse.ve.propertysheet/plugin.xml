<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.ve.propertysheet"
   name="%pluginName"
   version="1.0.1"
   provider-name="%providerName"
   class="org.eclipse.ve.internal.propertysheet.PSheetPlugin">

   <runtime>
      <library name="propertysheet.jar">
         <export name="*"/>
         <packages prefixes="org.eclipse.ve.internal.propertysheet"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.gef" optional="true"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.core.runtime"/>
   </requires>


</plugin>
